    "version": "0.2.0",
    "configurations": [
        {
            "name": "build_runner build",
            "request": "launch",
            "type": "dart",
            "program": "${workspaceFolder}/.dart_tool/build/entrypoint/build.dart",
            "args": ["build", "-d"]
        },
        {
            "name": "build_runner build (example)",
            "request": "launch",
            "type": "dart",
            "program": "${workspaceFolder}/example/.dart_tool/build/entrypoint/build.dart",
            "args": ["build", "-d"]
        },
        {
            "name": "flora_orm_example",
            "request": "launch",
            "program": "example/lib/main.dart",
            "type": "dart"
        },
        {
            "name": "flora_orm (profile mode)",
            "request": "launch",
            "type": "dart",
            "flutterMode": "profile"
        },
        {
            "name": "flora_orm (release mode)",
            "request": "launch",
            "type": "dart",
            "flutterMode": "release"
        },
        {
            "name": "example",
            "cwd": "example",
            "request": "launch",
            "type": "dart"
        },
        {
            "name": "example (profile mode)",
            "cwd": "example",
            "request": "launch",
            "type": "dart",
            "flutterMode": "profile"
        },
        {
            "name": "example (release mode)",
            "cwd": "example",
            "request": "launch",
            "type": "dart",
            "flutterMode": "release"
        }
    ]
}